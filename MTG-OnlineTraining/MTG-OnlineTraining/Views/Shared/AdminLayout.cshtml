     @*<script src="~/Training/js/jquery-3.6.0.min.js"></script>*@
     <script src="~/Training/js/jquery-3.5.1.min.js"></script>
     <script src="~/Training/js/jquery-ui.min.js"></script>
     <script src="~/Training/js/multiselect.min.js"></script>
